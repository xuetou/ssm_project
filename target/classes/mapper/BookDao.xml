<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.skynj.ssm.dao.BookDao">

	<select id="queryAll" resultType="com.skynj.ssm.pojo.Book">
		SELECT * from book ;
	</select>

	<select id="queryById" resultType="com.skynj.ssm.pojo.Book">
		select * from book where book_id=#{id}
	</select>
</mapper>
